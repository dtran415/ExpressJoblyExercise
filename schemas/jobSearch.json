{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "job title"
            ]
        },
        "minSalary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                100000
            ],
            "minimum": 0
        },
        "hasEquity": {
            "type": "boolean",
            "default": false,
            "title": "The hasEquity Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "title": "job title",
        "minSalary": 100000,
        "hasEquity": true
    }]
}