{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "job title"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                100000
            ],
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.1
            ],
            "maximum":1.0
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "c1"
            ]
        }
    },
    "examples": [{
        "title": "job title",
        "salary": 100000,
        "equity": 0.1,
        "companyHandle": "c1"
    }]
}